}

void loop(void) {
  frame++; 
  if (frame >= epd_bitmap_allArray_LEN) {   // pakai konstanta panjang array
    frame = 0;
  }

  u8g2.clearBuffer();	
  u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_allArray[frame]);
  u8g2.sendBuffer();

  delay(50); // beri jeda supaya animasi tidak terlalu cepat
}
